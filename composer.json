{
  "name": "conuti/edi-testfiles",
  "description": "EDI Testfiles",
  "type": "library",
  "prefer-stable": true,
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "vcs",
      "url": "git@gitlab.conuti.de:cu-powercloud/lib/coding-standard.git"
    },
    {
      "type": "vcs",
      "url": "git@gitlab.conuti.de:cu-powercloud/lib/utils-php.git"
    }
  ],
  "require": {
    "php": ">=8.1",
    "conuti/bo4e-php": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "phpstan/phpstan": "^1.9",
    "phpmd/phpmd": "^2.13",
    "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
    "friendsofphp/php-cs-fixer": "^3.16",
    "escapestudios/symfony2-coding-standard": "^3.13",
    "webservco/coding-standards": "dev-main",
    "conuti/coding-standard": "dev-main",
    "conuti/utils-php": "^0.0.5"
  },
  "autoload": {
    "psr-4": {
      "Conuti\\CDOC\\": "src/"
    }
  },
  "autoload-dev": {
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
